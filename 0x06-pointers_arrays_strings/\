#include "main.h"
/**
 * _putchar - writes the character c to stdout
 * @c: The character to print
 *
 * Return: On success 1.
 * On error, -1 is returned, and errno is set appropriately.
 */
char *_strncpy(char *dest, char *src, int n)
{
	int lenofsrc = 0;
	int lenofdest = 0;
	int i;

	for (i = 0; src[i] != '\0'; i++)
		lenofsrc++;
	for (i = 0; dest[i] != '\0'; i++)
		lenofdest++;
	for (i = 0; i < n && scr[i] != '\0'; i++)
		dest[i} = scr [i];
	return (dest);
